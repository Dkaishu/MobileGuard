<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        style="@style/TextTitle"
        android:text="设置中心"/>
    <com.fang.mobileguard.view.SettingItemView
        xmlns:mobileguard="http://schemas.android.com/apk/com.fang.mobileguard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/siv_update"
        mobileguard:destitle="自动更新设置"
        mobileguard:desoff="自动更新已关闭"
        mobileguard:deson="自动更新已开启" >
    </com.fang.mobileguard.view.SettingItemView>

    <com.fang.mobileguard.view.SettingItemView
        xmlns:mobileguard="http://schemas.android.com/apk/com.fang.mobileguard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/siv_address"
        mobileguard:destitle="归属地显示设置"
        mobileguard:desoff="归属地显示已关闭"
        mobileguard:deson="归属地显示已开启">
    </com.fang.mobileguard.view.SettingItemView>
    <com.fang.mobileguard.view.SettingClickView
        xmlns:mobileguard="http://schemas.android.com/apk/com.fang.mobileguard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/scv_toast_style">
    </com.fang.mobileguard.view.SettingClickView>
    <com.fang.mobileguard.view.SettingClickView
        xmlns:mobileguard="http://schemas.android.com/apk/com.fang.mobileguard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/scv_toast_location">
    </com.fang.mobileguard.view.SettingClickView>
    <com.fang.mobileguard.view.SettingItemView
        xmlns:mobileguard="http://schemas.android.com/apk/com.fang.mobileguard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/siv_blacknumber"
        mobileguard:destitle="黑名单设置"
        mobileguard:desoff="黑名单模式已关闭"
        mobileguard:deson="黑名单模式已开启">
    </com.fang.mobileguard.view.SettingItemView>
    <com.fang.mobileguard.view.SettingItemView
        xmlns:mobileguard="http://schemas.android.com/apk/com.fang.mobileguard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/siv_app_lock"
        mobileguard:destitle="程序锁设置"
        mobileguard:desoff="程序锁已关闭"
        mobileguard:deson="程序锁已开启">
    </com.fang.mobileguard.view.SettingItemView>

</LinearLayout>